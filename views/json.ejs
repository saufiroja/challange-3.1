<%- include("partials/header") %>

<div class="row justify-content-center" style="margin-top: 5rem">
  <div class="col-md-8">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">Id</th>
          <th scope="col">Username</th>
          <th scope="col">Password</th>
        </tr>
      </thead>
      <tbody>
        <% database.forEach((data, i) => { i++ %>
        <tr>
          <th scope="row"><%= i++ %></th>
          <td><%= data.id %></td>
          <td><%= data.username %></td>
          <td><%= data.password %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>

<%- include("partials/footer") %>
